{
  "name": "services-university-gateway-scheduler",
  "sourceRoot": "apps/services/university-gateway/scheduler/src",
  "projectType": "application",
  "prefix": "services-university-gateway-scheduler",
  "targets": {
    "build": {
      "executor": "./tools/executors/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/services/university-gateway/scheduler",
        "main": "apps/services/university-gateway/scheduler/src/main.ts",
        "tsConfig": "apps/services/university-gateway/scheduler/tsconfig.app.json",
        "maxWorkers": 2
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "services-university-gateway-scheduler:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/services/university-gateway/scheduler/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/apps/services/university-gateway/scheduler"],
      "options": {
        "jestConfig": "apps/services/university-gateway/scheduler/jest.config.js",
        "passWithNoTests": true
      }
    },
    "docker-express": {},
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn start services-university-gateway-backend",
          "echo \"Waiting for backend to start...\" && sleep 10",
          "yarn start services-university-gateway-scheduler"
        ],
        "parallel": true
      }
    }
  },
  "tags": ["scope:university-gateway-api"]
}
