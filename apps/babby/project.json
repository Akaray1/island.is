{
  "name": "babby",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/babby",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/babby"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/babby"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "babby:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "babby:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "babby:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "babby:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/babby/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/babby/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "dev-init": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["yarn get-secrets babby"],
        "parallel": false
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["./scripts/run-proxies.sh", "yarn start babby"],
        "parallel": true
      }
    }
  },
  "tags": []
}
