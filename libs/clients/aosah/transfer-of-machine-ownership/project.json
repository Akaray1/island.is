{
  "name": "clients-aosah-transfer-of-machine-ownership",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/clients/aosah/transfer-of-machine-ownership/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/clients/aosah/transfer-of-machine-ownership/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/clients/aosah/transfer-of-machine-ownership"
      ],
      "options": {
        "jestConfig": "libs/clients/aosah/transfer-of-machine-ownership/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "update-openapi-document": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "curl -H \"X-Road-Client: IS-DEV/GOV/10000/island-is-client\" http://localhost:8081/r1/IS-DEV/GOV/10013/Vinnueftirlitid-Protected/getOpenAPI?serviceCode=vinnuvelar-token > src/clientConfig.json",
          "cat <<< $(jq '.components.schemas.ProblemDetails.additionalProperties = false' src/clientConfig.json) > src/clientConfig.json",
          "cat <<< $(jq '.components.schemas.ValidationProblemDetails.additionalProperties = false' src/clientConfig.json) > src/clientConfig.json",
          "prettier --write src/clientConfig.json"
        ],
        "parallel": false,
        "cwd": "libs/clients/aosah/transfer-of-machine-ownership"
      }
    },
    "schemas/external-openapi-generator": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn openapi-generator -o libs/clients/aosah/transfer-of-machine-ownership/gen/fetch -i libs/clients/aosah/transfer-of-machine-ownership/src/clientConfig.json"
        ]
      }
    }
