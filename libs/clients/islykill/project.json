{
  "name": "clients-islykill",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/clients/islykill/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/clients/islykill/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/clients/islykill/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/libs/clients/islykill"]
    },
    "codegen/backend-client": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "mkdir -p tmp/ && touch tmp/islyklar.empty",
          "ISLYKILL_CERT=tmp/islyklar.empty yarn openapi-generator -o libs/clients/islykill/gen/fetch -i libs/clients/islykill/src/clientConfig.yaml"
        ],
        "parallel": false
      },
      "outputs": ["{projectRoot}/gen/fetch"]
    }
  },
  "tags": ["lib:client", "scope:client"]
}
