{
  "name": "clients-university-gateway-reykjavik-university",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/clients/university-gateway/reykjavik-university/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/clients/university-gateway/reykjavik-university/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/clients/university-gateway/reykjavik-university"
      ],
      "options": {
        "jestConfig": "libs/clients/university-gateway/reykjavik-university/jest.config.js",
        "passWithNoTests": true
      }
    },
    "schemas/external-openapi-generator": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn openapi-generator -o libs/clients/university-gateway/reykjavik-university/gen/fetch -i libs/clients/university-gateway/reykjavik-university/src/clientConfig.json"
        ]
      }
    }
  },
  "tags": ["lib:client", "scope:client"]
}
