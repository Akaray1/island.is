{
  "name": "form-system",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/portals/admin/form-system/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/portals/admin/form-system/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/portals/admin/form-system/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "codegen/frontend-client": {
      "executor": "nx:run-commands",
      "options": {
        "output": "libs/portals/admin/form-system/src/**/*.graphql.ts",
        "command": "graphql-codegen --config libs/portals/admin/form-system/codegen.yml"
      }
    }
  },
  "tags": []
}
