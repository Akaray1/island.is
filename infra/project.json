{
  "name": "infra",
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "charts": "node -r esbuild-register src/cli/generate-chart-values.ts",
    "cli": "node -r esbuild-register src/cli/cli.ts"
  },
  "sourceRoot": "infra/src",
  "projectType": "application",
  "generators": {},
  "targets": {
    "cli": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "node -r esbuild-register ./infra/src/cli/cli.ts"
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "{projectRoot}/jest.config.js"
      },
      "outputs": [
        "{workspaceRoot}/coverage/libs/{projectName}"
      ]
    },
    "charts": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "node -r esbuild-register ./infra/src/cli/generate-chart-values.ts"
          }
        ]
      }
    }
  },
  "tags": [
    "lib:nest",
    "lib:node",
    "scope:nest",
    "TODO:splitUp"
  ]
}
